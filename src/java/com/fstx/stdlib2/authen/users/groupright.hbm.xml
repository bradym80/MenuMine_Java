<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


  <class name="com.fstx.stdlib2.authen.users.GroupRight" table="groupright">
    <id name="id" column="groupRightsId" type="java.lang.Long" unsaved-value="null">
      <generator class="identity"/>
    </id>

	<property name="groupId" 
		column="groupRightGroupId"      
		type="java.lang.Long" 
		not-null="true" />

	<property name="rightCode" 
		column="groupRightsRightCode" 
		type="string" 
		not-null="true" />
	
  </class>

</hibernate-mapping>